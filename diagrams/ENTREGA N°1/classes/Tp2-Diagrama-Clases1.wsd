{
  + {static} Vertice(): Vertice
}

Tablero -> Vertice

class Terreno 
{
  + {static} Terreno(): Terreno
}

class Tablero 
{
  + {static} Tablero(): Tablero
  + tirarDados(): Integer
  + construirPoblado(jugador: Jugador, vertice: Vertice): void
  + construirCiudad(jugador: Jugador, vertice: Vertice): void
  + construirCarretera(jugador: Jugador, lado: Lado): void
  + granRutaComercial(jugador: Jugador, lado: Lado): void
